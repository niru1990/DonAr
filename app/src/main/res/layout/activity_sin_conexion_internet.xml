<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".sinConexionInternet">

    <include
        android:id="@+id/include3"
        layout="@layout/activity_don_ar_tool_bar"/>

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="303dp"
        android:layout_height="253dp"
        app:layout_constraintBottom_toBottomOf="@+id/include3"
        app:layout_constraintEnd_toEndOf="@+id/include3"
        app:layout_constraintStart_toStartOf="@+id/include3"
        app:layout_constraintTop_toTopOf="@+id/include3"
        app:srcCompat="@mipmap/sin_internet" />

    <TextView
        android:id="@+id/txtSinInternet"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/txtSinInternet3"
        android:textSize="21sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView" />

    <TextView
        android:id="@+id/txtSinInternetArriba"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/txtSinInternet1"
        android:textIsSelectable="false"
        android:textSize="21sp"
        app:layout_constraintBottom_toTopOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txtSinInterenetMedio"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/txtSinInternet2"
        android:textSize="21sp"
        app:layout_constraintBottom_toTopOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtSinInternetArriba" />

</androidx.constraintlayout.widget.ConstraintLayout>